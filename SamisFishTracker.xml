          <Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" offsetX="15" offsetY="50" />
        </Label>
        <Label name="$(parent)LabelBankRoe" width="100" height="12" font="ZoFontChat" inheritAlpha="true" color="FFFFFF"
